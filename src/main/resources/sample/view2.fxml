<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<VBox fx:id="vBox" alignment="center" spacing="10" xmlns:fx="http://javafx.com/fxml/1"
      xmlns="http://javafx.com/javafx/11" fx:controller="sample.View2Controller">
    <Button fx:id="button"  text="button"  disable="true"/>
    <HBox spacing="10" alignment="CENTER">
        <Button fx:id="enable"  text="enable"  onAction="#enable"/>
        <Button fx:id="disable" text="disable" onAction="#disable"/>    
    </HBox>
</VBox>
